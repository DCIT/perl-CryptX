#diag("samples_count=". @$data);

for my $h (@$data) {
  my $rsa_pri  = Crypt::PK::RSA->new->import_key(\pack("H*",$h->{PRI_DER}));
  my $rsa_pub  = Crypt::PK::RSA->new->import_key(\pack("H*",$h->{PUB_DER}));
  my $rsa_pri_h = $rsa_pri->key2hash;
  my $rsa_pub_h = $rsa_pub->key2hash;
  is($rsa_pri_h->{d}, uc $h->{PRI} =~ s/^0+//r, "$h->{PRI_FILE}/PRI");
  is($rsa_pri_h->{N}, uc $h->{PUB} =~ s/^0+//r, "$h->{PRI_FILE}/PUB");
  is($rsa_pub_h->{N}, uc $h->{PUB} =~ s/^0+//r, "$h->{PUB_FILE}/PUB");
  ok( $rsa_pub->verify_message(pack("H*", $h->{RSA_SHA1}), 'test-data', 'SHA1', 'v1.5'), "$h->{PRI_FILE}/RSA_SHA1");
  ok( $rsa_pub->verify_message(pack("H*", $h->{RSA_SHA256}), 'test-data', 'SHA256', 'v1.5'), "$h->{PRI_FILE}/RSA_SHA256");
}

done_testing();